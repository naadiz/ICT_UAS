<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".fragment.ProfileFragment">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#9E9E9E"
        android:text="Data anak ke-"
        android:id="@+id/tv_data_anak"
        android:textSize="17sp"
        app:layout_constraintStart_toStartOf="@+id/tv_nama_anak"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/product_sans_bold"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_nama_anak"
        android:layout_width="330dp"
        android:layout_height="51.43dp"
        android:background="@drawable/rectangle_one"
        android:fontFamily="@font/product_sans_regular"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:textColor="#9E9E9E"
        android:text="Sugeng Musyaffa"
        android:gravity="center_vertical"
        android:paddingLeft="20dp"
        android:textSize="15sp"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_data_anak"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="HardcodedText,MissingConstraints,NotSibling,RtlSymmetry" />

    <TextView
        android:id="@+id/tv_jenis_kelamin_anak"
        android:layout_width="330dp"
        android:layout_height="51.43dp"
        android:background="@drawable/rectangle_one"
        android:fontFamily="@font/product_sans_regular"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:textColor="#9E9E9E"
        android:text="Laki-laki"
        android:gravity="center_vertical"
        android:paddingLeft="20dp"
        android:textSize="15sp"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_nama_anak"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="HardcodedText,MissingConstraints,NotSibling,RtlSymmetry" />

    <TextView
        android:id="@+id/tv_ttl_anak"
        android:layout_width="330dp"
        android:layout_height="51.43dp"
        android:background="@drawable/rectangle_one"
        android:fontFamily="@font/product_sans_regular"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:textColor="#9E9E9E"
        android:text="28 Agustus 2022"
        android:gravity="center_vertical"
        android:paddingLeft="20dp"
        android:textSize="15sp"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_jenis_kelamin_anak"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="HardcodedText,MissingConstraints,NotSibling,RtlSymmetry" />
</androidx.constraintlayout.widget.ConstraintLayout>